{
  "name": "Valpineta",
  "displayName": "Valpineta",
  "expo": {
    "name": "Valpineta",
    "slug": "src",
    "scheme": "valpineta",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpg",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#191015"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "permissions": [
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.valpineta.app",
      "versionCode": 14,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon-android-adaptive-foreground.png",
        "backgroundImage": "./assets/images/app-icon-android-adaptive-background.png"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAhbzEPoqamBm-RVtvrDXUSF_nJpHh_bjQ"
        }
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.valpineta.app",
      "NSCameraUsageDescription": "This app uses the camera to take photos and videos."
    },
    "web": {
      "favicon": "./assets/images/logo.jpg"
    },
    "packagerOpts": {
      "sourceExts": [
        "js",
        "json",
        "ts",
        "tsx",
        "jsx, xml, gpx"
      ]
    },
    "plugins": [
      "expo-localization",
      "expo-image-picker",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": "true"
        }
      ]
    ]
  }
}